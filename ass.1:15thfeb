sno=int(input('Enter sno:'))
sname=input('Enter student name:')
group=input('Enter group:')
s1=int(input('Enter maths marks:'))
s2=int(input('Enter physics marks:'))
s3=int(input('Enter computersci.  marks:'))
total=s1+s2+s3
avg=total//3
if (avg>=90):
        print('First')
elif(avg>=80):
                print('Second')
elif(avg>=70):
                 print('Third')
else:
     print('Pass')
